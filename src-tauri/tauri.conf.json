{
  "build": {
    "beforeDevCommand": "npm run dev -- --host=localhost.atavismxi.com",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost.atavismxi.com:4321",
    "distDir": "../dist"
  },
  "package": {
    "productName": "Atavism XI Launcher",
    "version": "1.2.2"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      },
      "process": {
        "all": true
      },
      "os": {
        "all": true
      },
      "dialog": {
        "all": false,
        "ask": false,
        "confirm": false,
        "message": false,
        "open": true,
        "save": false
      },
      "fs": {
        "all": false,
        "readFile": true,
        "writeFile": false,
        "readDir": false,
        "copyFile": false,
        "createDir": true,
        "removeDir": false,
        "removeFile": false,
        "renameFile": false,
        "exists": true,
        "scope": ["$HOME/**"]
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.atavismxi.dev",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": true,
      "endpoints": [
        "https://gist.githubusercontent.com/Grahf0085/a8e43cc36f72a2bb30397c931440eb97/raw/"
      ],
      "dialog": false,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEYyNTg5Q0UwODI3MTkwOTAKUldTUWtIR0M0SnhZOGpvZkh6NmZMQ0ZwRjBaalNLdlAwMUp3YUY5UUJ4cXdmaHdUWjhLWGpCankK",
      "windows": {
        "installMode": "passive"
      }
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "AtavismXI",
        "width": 1536,
        "height": 864
      }
    ]
  }
}
